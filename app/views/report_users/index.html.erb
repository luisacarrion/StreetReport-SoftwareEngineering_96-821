<h1>Listing report_users</h1>

<table>
  <tr>
    <th>Rating</th>
    <th>Report</th>
    <th>User</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @report_users.each do |report_user| %>
  <tr>
    <td><%= report_user.rating %></td>
    <td><%= report_user.report_id %></td>
    <td><%= report_user.user_id %></td>
    <td><%= link_to 'Show', report_user %></td>
    <td><%= link_to 'Edit', edit_report_user_path(report_user) %></td>
    <td><%= link_to 'Destroy', report_user, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Report user', new_report_user_path %>
